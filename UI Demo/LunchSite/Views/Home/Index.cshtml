<div class="row" style="padding-top:20px;">

    <h2>Lunch Vote</h2>
    <p>Select your restaurant choice and click VOTE.</p>
    
    <form role="form">


        <!-- VOTE SUBMITTED -->
        <script type="text/javascript">
            function vote() {
                rv = 0;

                if (document.getElementById('r1').checked) { rv = 1; }
                else if (document.getElementById('r2').checked) { rv = 2; }
                else if (document.getElementById('r3').checked) { rv = 3; }
                else if (document.getElementById('r4').checked) { rv = 4; }

                if (rv != 0) {

                    $(".alert").hide();

                    $(".voteProg").animate({
                        width: "0%"
                    }, 1);

                    $(".voteProgBar").fadeIn(1);

                    $(".voteBar").fadeTo(500, 0.3);

                    $(".voteProg").animate({
                        width: "100%"
                    }, 1500, function () {
                        $(this).closest('.voteProgBar').fadeOut(1);
                        $(".voteBar").fadeTo(200, 1.00);
                        voteSubmitted(rv);
                        $(".alert-success").show();
                        $(".alert-success").fadeOut(6000);
                    }
                    );
                } else {
                    $(".alert-danger").show();
                    $(".alert-danger").fadeOut(6000);
                }
            }
        </script>

        <script type="text/javascript">
            function hideVote(){
                $(".voteProgBar").fadeTo(1, 0.00);
            }
        </script>


        <!-- VOTING BAR ANIMATIONS -->
        <script type="text/javascript">
            function voteUp(bar){
                if (bar == 1) {
                    $(".vote1").animate({
                        width: "100%"
                    }, 1000);
                } else if (bar == 2) {
                    $(".vote2").animate({
                        width: "100%"
                    }, 1000);
                } else if (bar == 3) {
                    $(".vote3").animate({
                        width: "100%"
                    }, 1000);
                } else if (bar == 4) {
                    $(".vote4").animate({
                        width: "100%"
                    }, 1000);
                }
            }
        </script>

        <script type="text/javascript">
            function voteDown(bar){
                if (bar == 1) {
                    $(".progress-bar.vote1").animate({
                        width: "1%"
                    }, 1000);
                } else if (bar == 2) {
                    $(".progress-bar.vote2").animate({
                        width: "1%"
                    }, 1000);
                } else if (bar == 3) {
                    $(".progress-bar.vote3").animate({
                        width: "1%"
                    }, 1000);
                } else if (bar == 4) {
                    $(".progress-bar.vote4").animate({
                        width: "1%"
                    }, 1000);
                }
            }
        </script>


        <script type="text/javascript">
            function voteSubmitted(bar){
                if (bar == 1) {
                    voteUp(1);
                    voteDown(2);
                    voteDown(3);
                    voteDown(4);
                } else if (bar == 2) {
                    voteDown(1);
                    voteUp(2);
                    voteDown(3);
                    voteDown(4);
                } else if (bar == 3) {
                    voteDown(1);
                    voteDown(2);
                    voteUp(3);
                    voteDown(4);
                } else if (bar == 4) {
                    voteDown(1);
                    voteDown(2);
                    voteDown(3);
                    voteUp(4);
                }
            }
        </script>      



        <!-- OPTION 1 -->
        <div class="row">
            <div class="radio">
                <div class="col-md-2">
                    <label><input type="radio" id="r1" name="optradio">Chipotle</label>
                </div>

                <div class="col-md-4">
                    <div class="progress voteBar">
                        <div class="progress-bar progress-bar-warning vote1" role="progressbar" aria-valuenow="60"
                             aria-valuemin="0" aria-valuemax="100" style="width: 1%;">

                            <span class="sr-only"></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- OPTION 2 -->
        <div class="row">
            <div class="radio">
                <div class="col-md-2">
                    <label><input type="radio" id="r2" name="optradio">Streets of NY</label>
                </div>

                <div class="col-md-4">
                    <div class="progress voteBar">
                        <div class="progress-bar progress-bar-warning vote2" role="progressbar" aria-valuenow="60"
                             aria-valuemin="0" aria-valuemax="100" style="width: 1%;">

                            <span class="sr-only"></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>


        <!-- OPTION 3 -->
        <div class="row">
            <div class="radio">
                <div class="col-md-2">
                    <label><input type="radio" id="r3" name="optradio">Panda Express</label>
                </div>

                <div class="col-md-4">
                    <div class="progress voteBar">
                        <div class="progress-bar progress-bar-warning vote3" role="progressbar" aria-valuenow="60"
                             aria-valuemin="0" aria-valuemax="100" style="width: 1%;">

                            <span class="sr-only"></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>


        <!-- OPTION 4 -->
        <div class="row">
            <div class="radio">
                <div class="col-md-2">
                    <label><input type="radio" id="r4" name="optradio">Some Burros</label>
                </div>

                <div class="col-md-4">
                    <div class="progress voteBar">
                        <div class="progress-bar progress-bar-warning vote4" role="progressbar" aria-valuenow="60"
                             aria-valuemin="0" aria-valuemax="100" style="width: 1%;">

                            <span class="sr-only"></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>


        <!-- VOTE PROGRESS BAR AND ALERTS -->
        <div class="row">
            <div class="col-md-1">
                <button type="button" onclick="vote()" class="btn btnPrice btn-primary">VOTE</button>
            </div>

            <div class="col-md-1">
                <div class="progress voteProgBar">
                    <div class="progress-bar progress-bar-success voteProg" role="progressbar" aria-valuenow="60"
                         aria-valuemin="0" aria-valuemax="100" style="width: 0%;">

                        <span class="sr-only"></span>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="alert alert-danger" role="alert">
                    <strong>ERROR:</strong> Nothing was selected. Please try again.
                </div>
                <div class="alert alert-success" role="alert">
                    <strong>VOTE SUBMITTED!</strong> Your vote was successful.
                </div>              
            </div>
        </div>
    </form>
</div>






